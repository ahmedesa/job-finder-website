{% extends 'base.html.twig' %}
{% block body %}
{% for category in categories %}
 <a href="{{ path('category.show', {slug: category.slug}) }}">{{ category.name }}</a>
{% include 'job/table.html.twig' with {
            'jobs': category.activeJobs|slice(0, max_jobs_on_homepage)
        } only %}
{% endfor %}
{% endblock %}